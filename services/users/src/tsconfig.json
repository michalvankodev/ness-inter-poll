{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": "lib",
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "ESNext",
    "noImplicitAny": false,
    "removeComments": true,
    "preserveConstEnums": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "suppressImplicitAnyIndexErrors": true,
    "noEmitHelpers": false,
    "typeRoots": ["node_modules/@types"],
    "lib": [
      "esnext"
    ]
  },
  "exclude": [
    "node_modules"
  ]
}
